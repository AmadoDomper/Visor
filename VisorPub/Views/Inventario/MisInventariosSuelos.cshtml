    let longitud = $("#txtLongitud");
    let latitud = $("#txtLatitud");
    let perfilModal = $("#txtPerfilModal");
    let nroCalicata = $("#txtNroCalicata");
    let msnm = $("#txtMsnm");
    let zonaMuestreo = $("#txtZonaMuestreo");
    let clasNatural = $("#txtClasNatural");
